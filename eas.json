{
  "cli": {
    "version": ">= 12.6.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": { "NODE_OPTIONS": "--max-old-space-size=4096" }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { "NODE_OPTIONS": "--max-old-space-size=4096" }
    },
    "preview": {
      "distribution": "internal",
      "env": { "NODE_OPTIONS": "--max-old-space-size=4096" }
    },
    "production": {
      "autoIncrement": true,
      "env": { "NODE_OPTIONS": "--max-old-space-size=4096" }
    }
  },
  "submit": {
    "production": {}
  }
}
